import numpy as np
import matplotlib.pyplot as plt
exploitation = np.random.rand(6, 4)
exploration = np.random.rand(6, 4) + exploitation
plt.subplot(1, 2, 1)
plt.imshow(exploitation, cmap='hot')
plt.title("Exploitation")
plt.subplot(1, 2, 2)
plt.imshow(exploration, cmap='hot')
plt.title("Exploration")
plt.show()
